//////esto no los dio por github xa empezar el ejercicio printDomTree////

function printDomTree() {
    var tree = ''
    var nonSemanticTags = [ 'div', 'span', 'script', 'style', 'meta', 'link', 'noscript'];
	

    function loop(elements, indent) {

	for (var i= 0; i< elements.length; i++) {
	var element = elements[i]

	if (!nonSemanticTags.includes(element.tagName.toLowerCase()))
	tree = tree + '\t'.repeat(indent) + element.tagName + '\n'

	loop(element.children, indent + 1)
	}
    }

	loop(document.children, 0)

}
printDomTree()

/*
HTML
 HEAD
  META
  META
 BODY
  SCRIPT
  ...
*/

// HINT recursion (in js)
